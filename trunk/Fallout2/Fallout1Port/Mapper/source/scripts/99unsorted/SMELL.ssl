#include "..\headers\define.h"

procedure start;


procedure start
begin
	if (script_action == 2) then begin// spatial_p_proc - Player is on or near hex of the location or object this script is using
		if source_obj == dude_obj then begin
			if (local_var(0) != 1) then begin
				if (is_success(do_check(dude_obj, STAT_pe, 0))) then begin
					display_msg(message_str(SCRIPT_SMELL, 100));
				end
				set_local_var(0, 1);
			end
		end
	end
end

