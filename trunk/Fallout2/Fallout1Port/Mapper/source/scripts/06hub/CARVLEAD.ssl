procedure pickup_p_proc
begin
	if source_obj > 0 then begin  SrcObj := 0;  SrcIsParty := 0;  SrcObj := obj_pid(source_obj);  if party_member_obj(SrcObj) then begin  SrcIsParty := 1;  end  end    if (source_obj == dude_obj) or (SrcIsParty == 1) then begin
		hostile := 1;
	end
end

procedure talk_p_proc
begin
	anim(dude_obj, 1000, rotation_to_tile(tile_num(dude_obj), tile_num(self_obj)));
	call get_reaction;
	if (local_var(7) == 4) then begin
		float_msg(self_obj, message_str(SCRIPT_CARVLEAD, random(143, 145)), 2);
	end
	else begin
		if (cur_map_index == 36) then begin //    THE HUB - ENTRANCE
		//	set_global_var(VAULT13_WATER_DAYS_LEFT, global_var(VAULT13_WATER_DAYS_LEFT) - 11);
		//	set_global_var(FOLLOWERS_INVADED_DATE, global_var(FOLLOWERS_INVADED_DATE) - 11);
		//	set_global_var(NECROPOLIS_INVADED_DATE, global_var(NECROPOLIS_INVADED_DATE) - 11);
		//	set_global_var(THE_HUB_INVADED_DATE, global_var(THE_HUB_INVADED_DATE) - 11);
		//	set_global_var(BROTHERHOOD_INVADED_DATE, global_var(BROTHERHOOD_INVADED_DATE) - 11);
		//	set_global_var(JUNKTOWN_INVADED_DATE, global_var(JUNKTOWN_INVADED_DATE) - 11);
		//	set_global_var(SHADY_SANDS_INVADED_DATE, global_var(SHADY_SANDS_INVADED_DATE) - 11);
		//	set_global_var(VAULT_13_INVADED_DATE, global_var(VAULT_13_INVADED_DATE) - 11);
			start_gdialog(817, self_obj, 4, -1, -1);
			gsay_start;
			if (local_var(7) == 3) then begin
				call HubAbandoned;
			end
			else begin
				if (local_var(7) == 5) then begin
					call HubLost;
				end
				else begin
					call CarvLead09;
				end
			end
			gsay_end;
			end_dialogue;
			if global_var(FADE_TIMES_INCREASED) then begin
				gfade_out(1);
				gfade_out(1);
			end
			gfade_out(1);
			set_local_var(5, 0);
			set_local_var(4, 0);
			if (metarule(22, 0) == 0) then begin    set_obj_visibility(self_obj, 1);    end//  MAKE INVISIBLE
			WaitForTwoHours := 0;
			gfade_in(1);
		end
		else begin
			if (local_var(7) == 5) then begin
				start_gdialog(817, self_obj, 4, -1, -1);
				gsay_start;
				call Lost;
				gsay_end;
				end_dialogue;
				if global_var(FADE_TIMES_INCREASED) then begin
					gfade_out(1);
					gfade_out(1);
				end
				gfade_out(1);
				set_local_var(5, 0);
				set_local_var(4, 0);
				WaitForTwoHours := 0;
				if (metarule(22, 0) == 0) then begin    set_obj_visibility(self_obj, 1);    end//  MAKE INVISIBLE
				gfade_in(1);
			end
			else begin
				if (local_var(4) == 0) then begin
					set_local_var(4, 1);
					start_gdialog(817, self_obj, 4, -1, -1);
					gsay_start;
					if (local_var(7) == 3) then begin
						set_local_var(7, 4);
						call Abandoned;
					end
					else begin
						call CarvLead00;
					end
					gsay_end;
					end_dialogue;
				end
				else begin
					start_gdialog(817, self_obj, 4, -1, -1);
					gsay_start;
					call CarvLead07;
					gsay_end;
					end_dialogue;
				end
			end
		end
	end
	if (UnloadCaravan == 1) then begin
		UnloadCaravan := 0;
		game_time_advance(60 * (60 * 10));
		item_caps_adjust(dude_obj, local_var(9));
		display_msg(message_str(SCRIPT_CARVLEAD, 520));
		display_msg(message_str(SCRIPT_CARVLEAD, 521) + local_var(9) + message_str(SCRIPT_CARVLEAD, 522));
		set_local_var(9, 0);
	end
end

procedure destroy_p_proc
begin

	if (cur_map_index != 36) then begin
		call remove_caravan;
	end
	if source_obj > 0 then begin  SrcObj := 0;  SrcIsParty := 0;  SrcObj := obj_pid(source_obj);  if party_member_obj(SrcObj) then begin  SrcIsParty := 1;  end  end    if (source_obj == dude_obj) or (SrcIsParty == 1) then begin
		if (((global_var(NUM_BAD_MONSTERS_KILLED) + global_var(NUM_GOOD_MONSTERS_KILLED)) >= 25) and ((global_var(NUM_GOOD_MONSTERS_KILLED) > (2 * global_var(NUM_BAD_MONSTERS_KILLED))) or (global_var(BERSERKER_REPUTATION) == 1))) then begin
			set_global_var(BERSERKER_REPUTATION, 1);
			set_global_var(CHAMPION_REPUTATION, 0);
