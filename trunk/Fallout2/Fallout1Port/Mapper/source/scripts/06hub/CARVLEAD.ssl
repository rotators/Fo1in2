procedure create_caravan
begin
	if (local_var(8) == 0) then begin
		set_local_var(8, 1);
		if (cur_map_index == 10) then begin //  JUNKTOWN - ENTRANCE, MORBID, LARS
			call Junktown_Caravan;
		end
		else begin
			if (cur_map_index == 13) then begin //  BROTHERHOOD - ENTRANCE
				call BOS_Caravan;
			end
			else begin
				if (cur_map_index == 4) then begin //  NECROPOLIS - HOTEL
					call Necropolis_Caravan;
				end
				else begin
					if (cur_map_index == 28) then begin //  L.A. BONEYARD - ADYTUM (ENTRANCE)
						call LABone_Caravan;
					end
				end
			end
		end
	end
end

procedure remove_caravan
begin
	if (local_var(8) == 1) then begin
		set_local_var(8, 0);
		if (cur_map_index == 10) then begin //  JUNKTOWN - ENTRANCE, MORBID, LARS
			Scenery_Creation_Ptr := tile_contains_pid_obj(30707, 0, PID_NS_RED_CARAVAN);
			destroy_object(Scenery_Creation_Ptr);
			Scenery_Creation_Hex := tile_num_in_direction(30707, 5, 2);
			if (PID_NS_RED_CARAVAN == PID_NS_RED_CARAVAN) then begin
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_RED_CARAVAN_HANDLE);
				destroy_object(Scenery_Creation_Ptr);
			end
			else begin
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_GREY_CARAVAN_HANDLE);
				destroy_object(Scenery_Creation_Ptr);
			end
			Scenery_Creation_Count := 0;
			while(Scenery_Creation_Count < 1) do begin
				Scenery_Creation_Hex := tile_num_in_direction(Scenery_Creation_Hex, 1, 1);
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_BLOCKING_HEX);
				destroy_object(Scenery_Creation_Ptr);
				Scenery_Creation_Count := Scenery_Creation_Count + 1;
			end
			Scenery_Creation_Count := 0;
			while(Scenery_Creation_Count < 3) do begin
				Scenery_Creation_Hex := tile_num_in_direction(Scenery_Creation_Hex, 2, 1);
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_BLOCKING_HEX);
				destroy_object(Scenery_Creation_Ptr);
				Scenery_Creation_Count := Scenery_Creation_Count + 1;
			end
			Scenery_Creation_Count := 0;
			while(Scenery_Creation_Count < 1) do begin
				Scenery_Creation_Hex := tile_num_in_direction(Scenery_Creation_Hex, 3, 1);
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_BLOCKING_HEX);
				destroy_object(Scenery_Creation_Ptr);
				Scenery_Creation_Count := Scenery_Creation_Count + 1;
			end
			Scenery_Creation_Count := 0;
			while(Scenery_Creation_Count < 1) do begin
				Scenery_Creation_Hex := tile_num_in_direction(Scenery_Creation_Hex, 4, 1);
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_BLOCKING_HEX);
				destroy_object(Scenery_Creation_Ptr);
				Scenery_Creation_Count := Scenery_Creation_Count + 1;
			end
			Scenery_Creation_Count := 0;
			while(Scenery_Creation_Count < 3) do begin
				Scenery_Creation_Hex := tile_num_in_direction(Scenery_Creation_Hex, 5, 1);
				Scenery_Creation_Ptr := tile_contains_pid_obj(Scenery_Creation_Hex, 0, PID_BLOCKING_HEX);
				destroy_object(Scenery_Creation_Ptr);
				Scenery_Creation_Count := Scenery_Creation_Count + 1;
			end
			Scenery_Creation_Ptr := tile_contains_pid_obj(30502, 0, PID_NS_GREY_CARAVAN);
			destroy_object(Scenery_Creation_Ptr);
			Scenery_Creation_Hex := tile_num_in_direction(30502, 5, 2);
