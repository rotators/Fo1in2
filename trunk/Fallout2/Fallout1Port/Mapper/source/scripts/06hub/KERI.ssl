procedure RecalcDate begin
	variable LVar0 := 0;
	variable LVar1 := 0;
	variable LVar2 := 0;

	LVar0 := get_day;
	if (LVar0 <= 3) then begin
		LVar0 := 3;
		LVar1 := get_month;
		LVar2 := game_time / (10 * 60 * 60 * 24 * 365);
	end
	else if ((LVar0 > 3) and (LVar0 <= 17)) then begin
		LVar0 := 17;
		LVar1 := get_month;
		LVar2 := game_time / (10 * 60 * 60 * 24 * 365);
	end
	else if (LVar0 > 17) then begin
		LVar0 := 3;
		LVar1 := get_month;
		if (LVar1 == 12) then begin
			LVar1 := 1;
			LVar2 := (game_time / (10 * 60 * 60 * 24 * 365)) + 1;
		end
		else begin
			LVar1 := LVar1 + 1;
			LVar2 := game_time / (10 * 60 * 60 * 24 * 365);
		end
	end
	set_local_var(9, LVar0);
	set_local_var(10, LVar1);
	set_local_var(11, LVar2);
end

