	LVar1 := roll_vs_skill(source_obj, SKILL_LOCKPICK, -10);
	LVar2 := do_check(source_obj, 1, 0);
	if (is_success(LVar0)) then begin
		if (is_critical(LVar0)) then begin
			call Locks_Check;
			Traps := 3;
		end
		else begin
			call Locks_Check;
			Traps := 2;
		end
	end
	else begin
		if (is_critical(LVar0)) then begin
			call Locks_Check;
			Traps := 0;
		end
		else begin
			call Locks_Check;
			Traps := 1;
		end
	end
end

